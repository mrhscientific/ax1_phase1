# Quick test case for transient sensitivity (very short simulation)
[controls]
eigmode alpha
dt 1.0e-4
t_end 0.001
hydro_per_neut 1
cfl 0.8
output_freq 5
output_file test_transient_sensitivity_quick
run_sensitivity true
sensitivity_output_file test_transient_sensitivity_quick_results
Sn 4

[geometry]
Nshell 5
G 2

[materials]
nmat 1

[material]
1

# group lines: imat g  sig_t  nu_sig_f  chi
[xs_group]
1 1  0.6   0.08  0.6
[xs_group]
1 2  0.5   0.04  0.4

# scatter: imat g' g  sigma_s(g'->g)
[scatter]
1 1 1  0.50
[scatter]
1 1 2  0.08
[scatter]
1 2 2  0.42

# delayed neutrons (6 groups): imat j beta lambda
[delayed]
1 1  0.0030  0.0124
[delayed]
1 2  0.0065  0.0305

[eos]
# i   a b c   Acv Bcv
1  1.0e6 0 1  1 0
5 1.0e6 0 1  1 0

[shells]
# i  r_out  mat  rho0  temp0
1   0.5  1  1.0  300.0
2   1.0  1  1.0  300.0
3   1.5  1  1.0  300.0
4   2.0  1  1.0  300.0
5   2.5  1  1.0  300.0

